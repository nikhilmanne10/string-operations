def shortestdistance(s,word1,word2):
     d1=-1,d2=-1;
     ans=100000;
for  i in range(len(s)):
    if(s[i]==word1):
       d1=i;
    if(s[i]==word2):
       d2=i;
    if(d1!=-1&&d2!=-1):
      ans=min(ans,abs(d1-d2))
return ans;
s=["hii","hello"];
word1="hi";
word2="hello"
print(shortestdistance(s,word1,word2));
